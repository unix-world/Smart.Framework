		<s:info>
			Type: DAV/Lock
			Method: [###DAV-METHOD|exml###]
			Path: [###DAV-REQ-PATH|exml###]
			Owner: [###DAV-AUTHOR|exml###]
		</s:info>
		<d:activelock>
			<d:locktype>
				<d:write/>
			</d:locktype>
			<d:lockscope>
