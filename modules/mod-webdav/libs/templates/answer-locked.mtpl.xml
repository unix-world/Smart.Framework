<?xml version="1.0" encoding="utf-8"?>
<d:lockinfo xmlns:d="DAV:" xmlns:s="SMARTFRAMEWORKWEBDAV:"[###DAV-XML-PREFIX###]>
	<d:lockdiscovery>
		<s:info>
			Type: DAV/Lock
			Method: [###DAV-METHOD|xml###]
			Path: [###DAV-REQ-PATH|xml###]
			Owner: [###DAV-AUTHOR|xml###]
		</s:info>
		<d:activelock>
			<d:locktype>
				<d:write/>
			</d:locktype>
			<d:lockscope>
				<d:exclusive/>
			</d:lockscope>
			<d:depth>[###LOCK-DEPTH|xml###]</d:depth>
			<d:owner>[###DAV-AUTHOR|xml###]</d:owner>
			<d:timeout>Second-[###LOCK-TIME-SEC|int###]</d:timeout>
			<d:locktoken>
				<d:href>urn:uuid:[###LOCK-UUID|xml###]</d:href>
			</d:locktoken>
			<d:lockroot>
				<d:href>[###DAV-REQ-PATH|xml###]</d:href>
			</d:lockroot>
		</d:activelock>
	</d:lockdiscovery>
</d:lockinfo>
