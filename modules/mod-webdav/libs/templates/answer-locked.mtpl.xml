			</d:lockscope>
			<d:depth>[###LOCK-DEPTH|exml###]</d:depth>
			<d:owner>[###DAV-AUTHOR|exml###]</d:owner>
			<d:timeout>Second-[###LOCK-TIME-SEC|int###]</d:timeout>
			<d:locktoken>
				<d:href>urn:uuid:[###LOCK-UUID|exml###]</d:href>
			</d:locktoken>
			<d:lockroot>
				<d:href>[###DAV-REQ-PATH|exml###]</d:href>
			</d:lockroot>
		</d:activelock>
	</d:lockdiscovery>
</d:lockinfo>
